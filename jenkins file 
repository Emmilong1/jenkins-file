pipeline {

  agent any

  stage{

    stage "("build") {
       
       steps{
          echo 'building the application...'
    }
}

 stage{

    stage ("test") {
       
       steps{
 echo 'testing the application...'

       }
    }  
 stage{

    stage {"("deployed") {
       
       steps{
 echo 'deployed the application...'
       }
    }  

       }
    }  
  }
}
